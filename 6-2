#include <stdio.h>
#include <ctype.h>
#include <string.h>
#include <math.h>

#define MAX 100
 int stack[MAX];
    int top = -1;
int evaluatePrefix(char* exp) {
    int len = strlen(exp);
    
    for (int i = len - 1; i >= 0; i--) {
        char c = exp[i];
        
        if (isdigit(c))
            stack[++top] = c - '0';
        else {
            int op1 = stack[top--];
            int op2 = stack[top--];
            
            switch (c) {
                case '+': stack[++top] = op1 + op2; break;
                case '-': stack[++top] = op1 - op2; break;
                case '*': stack[++top] = op1 * op2; break;
                case '/': stack[++top] = op1 / op2; break;
                case '^': stack[++top] = pow(op1, op2); break;
            }
        }
    }
    
    return stack[top];
}

int main() {
    char prefix[MAX];
    
    printf("Enter a prefix expression: ");
    gets(prefix);
    
    printf("Result of evaluation: %d\n", evaluatePrefix(prefix));
    
    return 0;
}
